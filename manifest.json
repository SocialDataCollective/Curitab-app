{
  "author": "SDC",
  "chrome_url_overrides": {
    "newtab": "override/index.html"
  },
  "background": {
    "scripts": ["js/lib/jQuery-2.1.3.min.js", "js/background.js"]
  },
  "content_scripts": [{
    "all_frames": true,
    "matches": ["<all_urls>", "http://*/*", "https://*/*"],
    "js": ["js/lib/jQuery-2.1.3.min.js", "js/change.js"],
    "run_at": "document_start"
  }],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "description": "Curitab presents users with curated images related to brands and asks interesting questions which are stored for improving market research.",
  "homepage_url": "http://socialdatacollective.com",
  "manifest_version": 2,
  "name": "CuriTab by SDC",
  "icons": {
    "128": "icons/C.jpg",
    "16": "icons/C.jpg",
    "48": "icons/C.jpg"
  },
  "oauth2": {
    "client_id": "796984726948-oen4vnot9l3jnksbee1njrqkbt4ehito.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/plus.login"
    ]
  },
  "permissions": ["http://*/*", "https://*/*", "identity", "identity.email",
    "tabs", "storage"
  ],
  "version": "0.1.0"
}
